<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
							xmlns="http://xmlns.jcp.org/xml/ns/javaee"
							xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
							xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
	<navigation-rule> 
		<from-view-id>*</from-view-id> 
		<navigation-case> 
			<from-outcome>general</from-outcome> 
			<to-view-id>/index.xhtml</to-view-id> 
		</navigation-case> 
	</navigation-rule> 
	<navigation-rule> 
		<from-view-id>*</from-view-id> 
		<navigation-case> 
			<from-outcome>infrastructure</from-outcome> 
			<to-view-id>/partials/content/initialState/infrastructure.xhtml</to-view-id> 
		</navigation-case> 
	</navigation-rule> 
	<navigation-rule> 
		<from-view-id>*</from-view-id> 
		<navigation-case> 
			<from-outcome>city</from-outcome> 
			<to-view-id>/partials/content/initialState/city.xhtml</to-view-id> 
		</navigation-case> 
	</navigation-rule> 
	<navigation-rule> 
		<from-view-id>*</from-view-id> 
		<navigation-case> 
			<from-outcome>busSystem</from-outcome> 
			<to-view-id>/partials/content/initialState/busSystem.xhtml</to-view-id> 
		</navigation-case> 
	</navigation-rule> 
	<navigation-rule> 
		<from-view-id>*</from-view-id> 
		<navigation-case> 
			<from-outcome>weather</from-outcome> 
			<to-view-id>/partials/content/initialState/weather.xhtml</to-view-id> 
		</navigation-case> 
	</navigation-rule>  
	<navigation-rule> 
		<from-view-id>*</from-view-id> 
		<navigation-case> 
			<from-outcome>sensors</from-outcome> 
			<to-view-id>/partials/content/initialState/sensors.xhtml</to-view-id> 
		</navigation-case> 
	</navigation-rule>  
	<navigation-rule> 
		<from-view-id>*</from-view-id> 
		<navigation-case> 
			<from-outcome>interaction</from-outcome> 
			<to-view-id>/partials/content/initialState/interaction.xhtml</to-view-id> 
		</navigation-case> 
	</navigation-rule> 
</faces-config>